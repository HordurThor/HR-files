
SELECT 'Customers: CID --> CN' AS FD,
CASE WHEN COUNT(*)=0 THEN 'MAY HOLD'
ELSE 'does not hold' END AS VALIDITY
FROM (
    SELECT R.CID
    FROM Customers R
    GROUP BY R.CID
    HAVING COUNT(DISTINCT R.CN) > 1
) X;


SELECT 'Customers: CID --> CS' AS FD,
